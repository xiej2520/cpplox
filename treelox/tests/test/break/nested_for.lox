	for (var j = i - 1;; j = j - 2) {
		print j;
		if (j < 6) {
			break;
		}
	}
	for (; i > 5;) {
		var j = 10;
		while (j > 9) {
			print j;
			break;
		}
		print i;
		i = i - 3;
	}
	if (i < 2) {
		print i;
		break;
	}
}

